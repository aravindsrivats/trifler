doctype
html
  head
    meta(http-equiv='content-type', content='text/html; charset=UTF-8')
    title !{title}
    each css in stylesheets
      link(href=css, rel='stylesheet')
    style !{css}
    each lib in scripts
      script(type='text/javascript', src=lib)
  body !{html}
    script(type='text/javascript') !{js}
